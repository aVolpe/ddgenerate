<rdf:RDF
        xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:dc="http://purl.org/dc/elements/1.1/"
        xmlns:llamado="http://www.dncp.gov.py.com/llamado#"
        xmlns:th="http://www.thymeleaf.org">

    <owl:Class th:each="clazz: ${classes}" th:attr="'rdf:ID'=${clazz.getOwlName()}">
        <rdfs:comment xml:lang="en" th:text="${clazz.descripcionIngles}">#</rdfs:comment>
        <rdfs:comment xml:lang="es" th:text="${clazz.descripcionEspanhol}">#</rdfs:comment>
        <rdfs:label xml:lang="en" th:text="${clazz.labelIngles}">#</rdfs:label>
        <rdfs:label xml:lang="es" th:text="${clazz.labelEspanhol}">#</rdfs:label>
    </owl:Class>


    <owl:DatatypeProperty th:each="property: ${properties}" th:attr="'rdf:ID'=${property.getOwlName()}">
        <rdfs:domain th:each="clazz: ${property.classes}" th:attr="'rdf:resource'='#' + ${clazz.getOwlName()}"/>
        <rdfs:range th:attr="'rdf:resource'=${property.tipo}"/>
        <rdfs:comment xml:lang="en" th:text="${property.descripcionIngles}">#</rdfs:comment>
        <rdfs:comment xml:lang="es" th:text="${property.descripcionEspanhol}">#</rdfs:comment>
        <rdfs:label xml:lang="en" th:each="label: ${property.labelsIngles}" th:text="${label}">#</rdfs:label>
        <rdfs:label xml:lang="es" th:each="label: ${property.labelsEspanhol}" th:text="${label}">#</rdfs:label>
    </owl:DatatypeProperty>

</rdf:RDF>